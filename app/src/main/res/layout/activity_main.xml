<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.muroigundan.task_app.MainActivity">

    <Button
        android:id="@+id/button1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="7"
        android:onClick="RegiSend_onClick1"
        tools:layout_editor_absoluteX=""
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_weight="2"
        android:onClick="RegiSend_onClick2"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="456dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:onClick="RegiSend_onClick3"
        tools:layout_editor_absoluteX="312dp"
        tools:layout_editor_absoluteY="456dp" />

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        tools:context="com.muroigundan.task_app.MainActivity">

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="ListSend_onClick"
            android:text="@string/more"
            tools:layout_editor_absoluteX="200dp"
            tools:layout_editor_absoluteY="647dp" />

        <Button
            android:id="@+id/button5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="CheerPage_onClick"
            android:text="応援ページ"
            tools:layout_editor_absoluteX="200dp"
            tools:layout_editor_absoluteY="647dp" />
    </LinearLayout>

</LinearLayout>
